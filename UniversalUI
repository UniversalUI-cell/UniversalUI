local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("UNIVERSAL UI", "DarkTheme")


local Universal = Window:NewTab("Universal")
local UniversalSection = Universal:NewSection("Universal")


UniversalSection:NewButton("Infinity yield", "launches infinite yield", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
end)


UniversalSection:NewButton("Sirius Universal", "Universal", function()
   loadstring(game:HttpGet('https://sirius.menu/script'))()
end)


UniversalSection:NewButton("QP HUB (key:QP)", "Universal script updates everyday", function()
   loadstring(game:HttpGet("https://raw.githubusercontent.com/QP-Community/Roblox-Exploit/main/Loader.lua"))()
end)


local Games = Window:NewTab("Games")
local GamesSection = Games:NewSection("Games")


GamesSection:NewButton("Arsenal", "Arsenal script", function()
   lrm_load_script("41f7d88bf584cfbb05c0f507eda3a130")
end)


GamesSection:NewButton("Fnaf Doom", "Animatronics ESP (NOTE!: only works in fnaf 2)", function()
   loadstring(game:HttpGet("https://raw.githubusercontent.com/liigmabalz/Projetos_Lua/main/FNaF_Doom2.lua"))();
end)


GamesSection:NewButton("Blade ball", "launches Blade ball script", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/FFJ1/Roblox-Exploits/main/scripts/BladeBallV3.lua"))()
end)


local Credits = Window:NewTab("Credits")
local CreditsSection = Credits:NewSection("Credits to...")


CreditsSection:NewLabel(".zzynic")

CreditsSection:NewLabel("and")

CreditsSection:NewLabel("mirtis1")
